<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#2f3546"/>
  
  <!-- Compass design -->
  <circle cx="400" cy="300" r="180" fill="none" stroke="#cbb491" stroke-width="4"/>
  <circle cx="400" cy="300" r="160" fill="none" stroke="#cbb491" stroke-width="2" stroke-dasharray="10,5"/>
  
  <!-- Compass directions -->
  <line x1="400" y1="120" x2="400" y2="480" stroke="#cbb491" stroke-width="3"/>
  <line x1="220" y1="300" x2="580" y2="300" stroke="#cbb491" stroke-width="3"/>
  
  <!-- Compass points -->
  <polygon points="400,100 410,130 390,130" fill="#cbb491"/>
  <polygon points="600,300 570,310 570,290" fill="#cbb491"/>
  <polygon points="400,500 390,470 410,470" fill="#cbb491"/>
  <polygon points="200,300 230,290 230,310" fill="#cbb491"/>
  
  <!-- Center star -->
  <g transform="translate(400, 300)">
    <path d="M0,-20 L5,-5 L20,0 L5,5 L0,20 L-5,5 L-20,0 L-5,-5 Z" fill="#cbb491"/>
  </g>
  
  <!-- Decorative elements -->
  <circle cx="400" cy="300" r="30" fill="none" stroke="#cbb491" stroke-width="2"/>
  <circle cx="400" cy="300" r="10" fill="#cbb491"/>
  
  <!-- Direction labels -->
  <text x="400" y="100" text-anchor="middle" font-size="16" fill="#e9e6df" font-weight="bold">N</text>
  <text x="610" y="305" text-anchor="middle" font-size="16" fill="#e9e6df" font-weight="bold">E</text>
  <text x="400" y="510" text-anchor="middle" font-size="16" fill="#e9e6df" font-weight="bold">S</text>
  <text x="190" y="305" text-anchor="middle" font-size="16" fill="#e9e6df" font-weight="bold">W</text>
  
  <!-- Title -->
  <text x="400" y="420" text-anchor="middle" font-size="24" fill="#e9e6df" font-weight="bold">Why Choose</text>
  <text x="400" y="450" text-anchor="middle" font-size="24" fill="#e9e6df" font-weight="bold">Shared Sparks</text>
</svg> 
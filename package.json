{
  "name": "shared_sparks",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && NODE_OPTIONS='--max_old_space_size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next build",
    "build:dev": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next build --no-lint",
    "build:skip-static": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit",
    "lighthouse": "lhci autorun",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 'cypress run --record'",
    "test:pact": "jest --config jest.config.js --testMatch '**/pact/**/*.test.ts'",
    "test:pact:watch": "jest --config jest.config.js --testMatch '**/pact/**/*.test.ts' --watch",
    "pact:publish": "pact-broker publish ./pacts --broker-base-url=$PACT_BROKER_URL --broker-token=$PACT_BROKER_TOKEN --consumer-version=$GIT_COMMIT"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.8",
    "cloudinary": "^2.5.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.4.7",
    "ioredis": "^5.5.0",
    "lucide-react": "^0.483.0",
    "next": "^15.2.0",
    "next-auth": "^4.24.11",
    "preline": "^3.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "@upstash/redis": "^1.28.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.6.0",
    "@lhci/cli": "^0.12.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prisma": "^6.5.0",
    "snyk": "^1.1500.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2",
    "cypress": "^13.6.6",
    "cypress-axe": "^1.0.0",
    "start-server-and-test": "^2.0.3",
    "@pact-foundation/pact": "^17.0.0",
    "@pact-foundation/pact-node": "^12.0.0",
    "pact-broker-client": "^1.0.0"
  },
  "overrides": {
    "esbuild": "0.18.20",
    "esbuild-register": {
      "esbuild": "0.18.20"
    },
    "prisma": {
      "esbuild": "0.18.20"
    }
  },
  "resolutions": {
    "esbuild": "0.18.20"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
  }
}

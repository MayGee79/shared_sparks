{
  "name": "shared_sparks",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && NODE_OPTIONS='--max_old_space_size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next build",
    "build:dev": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next build --no-lint",
    "build:skip-static": "prisma generate && cross-env NODE_OPTIONS='--max_old_space_size=4096' next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit",
    "lighthouse": "lhci autorun",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 'cypress run --record'"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.1.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.10.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.1",
    "cloudinary": "^2.0.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^11.0.5",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.330.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "preline": "^2.0.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@upstash/redis": "^1.28.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.17",
    "css-loader": "^6.10.0",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.6.0",
    "@lhci/cli": "^0.12.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prisma": "^5.10.0",
    "snyk": "^1.1278.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.5.0",
    "start-server-and-test": "^2.0.3"
  },
  "overrides": {
    "esbuild": "0.18.20",
    "esbuild-register": {
      "esbuild": "0.18.20"
    },
    "prisma": {
      "esbuild": "0.18.20"
    }
  },
  "resolutions": {
    "esbuild": "0.18.20"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
  }
}
